          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/react-app1/.eslintrc",
            "tsConfig": [
              "apps/react-app1/tsconfig.app.json",
              "apps/react-app1/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/react-app1/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/react-app1/jest.config.js",
            "tsConfig": "apps/react-app1/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
