{
  "version": 1,
  "projects": {
    "shared-utils": {
      "root": "libs/shared-utils",
      "sourceRoot": "libs/shared-utils/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/shared-utils/.eslintrc",
            "tsConfig": [
              "libs/shared-utils/tsconfig.lib.json",
              "libs/shared-utils/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/shared-utils/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/shared-utils/jest.config.js",
            "tsConfig": "libs/shared-utils/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "react-app1": {
      "root": "apps/react-app1",
      "sourceRoot": "apps/react-app1/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/react-app1",
            "index": "apps/react-app1/src/index.html",
            "main": "apps/react-app1/src/main.tsx",
            "polyfills": "apps/react-app1/src/polyfills.ts",
            "tsConfig": "apps/react-app1/tsconfig.app.json",
            "assets": [
              "apps/react-app1/src/favicon.ico",
              "apps/react-app1/src/assets"
            ],
            "styles": ["apps/react-app1/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/react-app1/src/environments/environment.ts",
                  "with": "apps/react-app1/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "react-app1:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "react-app1:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/react-app1/.eslintrc",
            "tsConfig": [
              "apps/react-app1/tsconfig.app.json",
              "apps/react-app1/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/react-app1/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/react-app1/jest.config.js",
            "tsConfig": "apps/react-app1/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "react-app2": {
      "root": "apps/react-app2",
      "sourceRoot": "apps/react-app2/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/react-app2",
            "index": "apps/react-app2/src/index.html",
            "main": "apps/react-app2/src/main.tsx",
            "polyfills": "apps/react-app2/src/polyfills.ts",
            "tsConfig": "apps/react-app2/tsconfig.app.json",
            "assets": [
              "apps/react-app2/src/favicon.ico",
              "apps/react-app2/src/assets"
            ],
            "styles": ["apps/react-app2/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/react-app2/src/environments/environment.ts",
                  "with": "apps/react-app2/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "react-app2:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "react-app2:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/react-app2/.eslintrc",
            "tsConfig": [
              "apps/react-app2/tsconfig.app.json",
              "apps/react-app2/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/react-app2/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/react-app2/jest.config.js",
            "tsConfig": "apps/react-app2/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      },
      "component": {
        "style": "css"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "react-app1"
}
